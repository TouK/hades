-- DROP TABLE IF EXISTS HADES_RESULTS;
CREATE TABLE HADES_RESULTS (
  DS                    VARCHAR(128)  NOT NULL,
  SQL_STMT              VARCHAR(100)  NOT NULL,
  EXEC_TIMEOUT          INT           NOT NULL,
  FORCING_PERIOD_MILLIS INT           NOT NULL,
  CONN_TIMEOUT          INT           NOT NULL,
  TS                    TIMESTAMP     NOT NULL,
  INSTANCE_ID           VARCHAR(128)  NOT NULL,
  LOG_PREFIX            VARCHAR(128)  NOT NULL,
  TIME                  INT           NOT NULL,

  PRIMARY KEY (DS, SQL_STMT, EXEC_TIMEOUT, FORCING_PERIOD_MILLIS, CONN_TIMEOUT, TS)
);

